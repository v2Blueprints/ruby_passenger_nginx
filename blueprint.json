{
	"about": {
		"title": "Rails",
		"explanation": "nginx passenger example"
	},
	"binding_target": {
		"user": "www",
		"port": "8889",
		"servername": "ruby.^^domain_name^^",
		"ServerAdmin": "ruby Admin",
		"user_home": "/var/www/",
		"code_destination": "/var/www/^^identifer^^"
	},
	"system_packages": {
		"adds": [
			"nginx",
			"libnginx-mod-http-passenger"
		]
	},
	"bindings": [
		{
			"type": "embed",
			"descriptor": {
				"repository": "https://github.com/v2Blueprints/ruby-rbenv"
			},
			"user": "^^user^^"
		},
		{
      "type": "embed",
      "runtimes": [
        "lxd"
      ],
      "target": {
        "repository": "https://github.com/v2Blueprints/enginesd_debian_base"
      }
    }
	],
	"images": [
    {
      "runtimes": [
        "docker"
      ],
      "target": {
        "repository": "https://github.com/v2Blueprints/enginesd_debian_base"
      }
    }
  ]
}
